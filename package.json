{
    "name": "exact-print",
    "version": "2.0.0",
    "author": "Kai-Oliver Prott <stu126209@mail.uni-kiel.de>",
    "synopsis": "Libaries to exactly print curry-ast",
    "category": [ "Metaprogramming" ],
    "compilerCompatibility": {
        "pakcs": ">= 3.8.0, < 4.0.0",
        "kics2": ">= 3.3.0, < 4.0.0"
    },
    "dependencies": {
        "base"          : ">= 3.3.0, < 4.0.0",
        "currypath"     : ">= 3.0.0, < 4.0.0",
        "curry-ast"     : ">= 3.1.0, < 4.0.0",
        "frontend-exec" : ">= 3.3.0, < 4.0.0",
        "read-legacy"   : ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "Curry.Comment",
                         "Curry.ExactPrint",
                         "Curry.ExactPrintClass" ],
    "source": {
        "git": "https://github.com/curry-packages/exact-print.git",
        "tag": "$version"
    },
    "testsuite": {
        "src-dir": "examples",
        "modules": [ "TestExactPrint" ]
    }
}
